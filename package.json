{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "preview": "vite preview",
    "version:patch": "yarn standard-version -r patch",
    "version:minor": "yarn standard-version -r minor",
    "version:major": "yarn standard-version -r major",
    "version:prepatch": "yarn standard-version -r patch -t alpha-v",
    "version:preminor": "yarn standard-version -r minor -t alpha-v",
    "version:premajor": "yarn standard-version -r major -t alpah-v",
    "version:prerelease": "yarn standard-version --prerelease alpha",
    "lint": "eslint './src/**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './src/**/*.{ts,tsx,js,jsx}'",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "style": "yarn format && yarn lint",
    "style:fix": "yarn format:fix && yarn lint:fix",
    "storybook:start": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.1",
    "@mui/system": "^5.6.1",
    "dexie": "^3.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@emotion/babel-plugin": "^11.9.2",
    "@storybook/addon-a11y": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.28",
    "@storybook/react": "^6.4.22",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "@vitejs/plugin-react": "^1.3.0",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.0.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3",
    "vite": "^2.9.2"
  }
